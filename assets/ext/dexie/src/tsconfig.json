{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2020",
    "module": "ES2020",
    "importHelpers": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "forceConsistentCasingInFileNames": true,
    "outDir": "../build/",
    "sourceMap": true
  },
  "dirs": [
    "."
  ]
}
