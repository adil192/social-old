{
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "removeComments": true,
    "noEmitOnError": true,
    "strict": false,
    "alwaysStrict": false,
    "noImplicitAny": false,
    "typeRoots": ["node_modules/@types/"],
    "lib": [
      "es2020",
      "webworker",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
